<%= form_for(@order) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :credit_card_number %>
    <%= f.text_field :credit_card_number %>
    <div id="credit_card_number_error"></div>
  </div>
  <div class="field">
    <%= f.label :credit_card_expires_on %>
    <%= f.date_select :credit_card_expires_on %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>